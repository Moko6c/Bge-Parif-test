$( document ).ready(function() {
    $('#DonneesSociales').on('click', function (){
        $('#donsociales').toggle();
    });
    $('#SortieCouveuse').on('click', function (){
        $('#SortCouveuse').toggle();
    });


    $('#antenne').on('change', function(){
        var antenneSelect = $('#antenne').val();
        console.log(antenneSelect)
        switch (antenneSelect) {
            case 'paris11':               
                chart1.load({
                    url: 'data/Paris11.csv'
                    //unload: chart.columns,
                });
                chart2.load({
                    url: 'data/Paris11.csv'
                    //unload: chart.columns,
                });
                chart3.load({
                    url: 'data/Paris11.csv'
                    //unload: chart.columns,
                });
                chart4.load({
                    url: 'data/Paris11.csv'
                    //unload: chart.columns,
                });
                chart5.load({
                    url: 'data/Paris11.csv'
                    //unload: chart.columns,
                });
                chart6.load({
                    url: 'data/Paris11.csv'
                    //unload: chart.columns,
                });
                chart7.load({
                    url: 'data/Paris11.csv'
                    //unload: chart.columns,
                });
                $('a path').removeClass();
                $('#FR-75_1_').addClass('active');
                //$('label').prepend($('<h2></h2>').addClass('titre-antenne').text(antenneSelect));
                $('.titre-antenne').text(antenneSelect);
                break;
            case 'paris14':
                //var data=[["Homme", 31], ["Femme", 69]];
                chart1.load({
                    url: 'data/Paris14.csv'
                    //unload: chart.columns,
                });
                chart2.load({
                    url: 'data/Paris14.csv'
                    //unload: chart.columns,
                });
                chart3.load({
                    url: 'data/Paris14.csv'
                    //unload: chart.columns,
                });
                chart4.load({
                    url: 'data/Paris14.csv'
                    //unload: chart.columns,
                });
                chart5.load({
                    url: 'data/Paris14.csv'
                    //unload: chart.columns,
                });
                chart6.load({
                    url: 'data/Paris14.csv'
                    //unload: chart.columns,
                });
                chart7.load({
                    url: 'data/Paris14.csv'
                    //unload: chart.columns,
                });
                $('a path').removeClass();
                $('#FR-75_1_').addClass('active');
                $('.titre-antenne').text(antenneSelect);
                break;
            case 'paris18':
                var data=[["Homme", 22],["Femme", 78]];
